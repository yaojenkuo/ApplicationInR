mosaicPart <- function(img,rows,cols,q) {
lrows <- length(rows)
lcols <- length(cols)
newImg <- img
randomNoise <- matrix(nrow=lrows, ncol=ncols,runif(lrows*lcols))
newImg@grey <- (1-q) * img@grey + q * randomNoise
return(newImg)
}
